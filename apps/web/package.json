    "@vitest/coverage-v8": "^1.6.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "jsdom": "^24.0.0",
    "lovable-tagger": "^1.1.9",
    "postcss": "^8.4.47",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.5.3",
    "vite": "^5.4.1",
    "vitest": "^1.6.0"
